	stem21: 21 stem22: 22 stem23: 23 stem24: 24 stem25: 25 stem26: 26 stem27: 27 stem28: 28 stem29: 29 stem30: 30 \
	stem31: 31 stem32: 32 stem33: 33 stem34: 34 stem35: 35 \
	stemEnd: \
	boldStemStart: \
