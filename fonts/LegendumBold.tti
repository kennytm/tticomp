	boldStem30: 30 boldStem31: 31 boldStem32: 32 boldStem33: 33 boldStem34: 34 boldStem35: 35 boldStem36: 36 boldStem37: 37 boldStem38: 38 \
	boldStem39: 39 boldStem40: 40 boldStem41: 41 boldStem43: 43 boldStem45: 45 \
